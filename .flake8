[flake8]
exclude = venv, __init__.py, doc/_build, .venv
select = W191, W291, W293, W391, E115, E117, E122, E124, E125, E225, E231, E301, E303, E501, F401, F403, N801, N802, N803, N804, N805, N806, N807, N811, N812, N813, N814, N815, N816, N817, N818
count = True
max-complexity = 10
max-line-length = 99
statistics = True
